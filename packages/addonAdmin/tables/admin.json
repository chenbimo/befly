{
    "nickname": {
        "name": "昵称",
        "type": "varchar",
        "min": 1,
        "max": 50
    },
    "username": {
        "name": "用户名",
        "type": "varchar",
        "input": "@alphanumeric_",
        "min": 2,
        "max": 30
    },
    "password": {
        "name": "密码",
        "type": "varchar",
        "min": 6,
        "max": 500
    },
    "email": {
        "name": "邮箱",
        "type": "varchar",
        "input": "@email",
        "max": 100
    },
    "phone": {
        "name": "手机号",
        "type": "varchar",
        "input": "@phone",
        "max": 20
    },
    "avatar": {
        "name": "头像",
        "type": "varchar",
        "max": 500
    },
    "roleCode": {
        "name": "角色编码",
        "type": "varchar",
        "input": "@alphanumeric_",
        "min": 2,
        "max": 50
    },
    "roleType": {
        "name": "角色类型",
        "type": "varchar",
        "input": "admin|user",
        "min": 4,
        "max": 5,
        "default": "user",
        "index": true
    },
    "lastLoginTime": {
        "name": "最后登录时间",
        "type": "bigint",
        "input": "number",
        "default": 0
    },
    "lastLoginIp": {
        "name": "最后登录IP",
        "type": "varchar",
        "max": 50
    }
}
