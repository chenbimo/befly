{
    "name": {
        "name": "接口名称",
        "type": "string",
        "min": 2,
        "max": 100,
        "index": true
    },
    "path": {
        "name": "接口路径",
        "type": "string",
        "min": 1,
        "max": 200,
        "index": true
    },
    "method": {
        "name": "请求方法",
        "type": "string",
        "min": 3,
        "max": 10,
        "default": "POST",
        "index": true,
        "regexp": "^(GET|POST|PUT|DELETE|PATCH)$"
    },
    "description": {
        "name": "接口描述",
        "type": "string",
        "min": 0,
        "max": 500
    },
    "addonName": {
        "name": "所属插件",
        "type": "string",
        "min": 0,
        "max": 50
    },
    "addonTitle": {
        "name": "插件标题",
        "type": "string",
        "min": 0,
        "max": 100
    }
}
