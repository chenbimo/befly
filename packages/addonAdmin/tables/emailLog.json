{
    "adminId": {
        "name": "发送人ID",
        "type": "bigint",
        "input": "number",
        "min": 0,
        "index": true
    },
    "username": {
        "name": "发送人账号",
        "type": "varchar",
        "max": 100
    },
    "nickname": {
        "name": "发送人昵称",
        "type": "varchar",
        "max": 100
    },
    "toEmail": {
        "name": "收件人邮箱",
        "type": "varchar",
        "input": "@email",
        "min": 5,
        "max": 200
    },
    "subject": {
        "name": "邮件主题",
        "type": "varchar",
        "min": 1,
        "max": 200
    },
    "content": {
        "name": "邮件内容",
        "type": "text"
    },
    "ccEmail": {
        "name": "抄送邮箱",
        "type": "varchar",
        "max": 500
    },
    "bccEmail": {
        "name": "密送邮箱",
        "type": "varchar",
        "max": 500
    },
    "sendTime": {
        "name": "发送时间",
        "type": "bigint",
        "input": "number"
    },
    "sendResult": {
        "name": "发送结果",
        "type": "bigint",
        "input": "number",
        "min": 0,
        "max": 1,
        "default": 0
    },
    "messageId": {
        "name": "消息ID",
        "type": "varchar",
        "max": 200
    },
    "failReason": {
        "name": "失败原因",
        "type": "varchar",
        "max": 500
    }
}
