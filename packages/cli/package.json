{
    "name": "befly-cli",
    "version": "3.7.5",
    "description": "Befly 框架命令行工具",
    "type": "module",
    "bin": {
        "befly": "./bin/index.ts"
    },
    "exports": {
        ".": "./bin/index.ts"
    },
    "files": [
        "bin/",
        "commands/",
        "report/",
        "util/",
        "README.md",
        "LICENSE",
        "package.json",
        "tsconfig.json",
        "types.d.ts",
        "util.ts"
    ],
    "scripts": {
        "build": "echo 'No build needed for CLI'"
    },
    "keywords": [
        "befly",
        "cli",
        "framework",
        "command-line"
    ],
    "author": "chenbimo",
    "license": "MIT",
    "dependencies": {
        "befly": "workspace:*",
        "es-toolkit": "^1.41.0",
        "pathe": "^1.1.2"
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "engines": {
        "node": ">=24.0.0",
        "bun": ">=1.3.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
