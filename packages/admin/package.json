{
    "name": "befly-admin",
    "version": "3.4.5",
    "description": "Befly Admin - 基于 Vue3 + OpenTiny Vue 的后台管理系统",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "src",
        "public",
        "index.html",
        "vite.config.ts",
        "tsconfig.json",
        ".env",
        ".env.development",
        ".env.production",
        ".gitignore",
        "bunfig.toml",
        "README.md"
    ],
    "scripts": {
        "dev": "befly-vite dev",
        "build": "vue-tsc && befly-vite build",
        "preview": "befly-vite preview",
        "type-check": "vue-tsc --noEmit",
        "sync:admin": "bunx befly sync:admin"
    },
    "dependencies": {
        "@opentiny/vue": "^3.27.0",
        "axios": "^1.13.2",
        "pinia": "^3.0.4",
        "vue": "^3.5.22",
        "vue-router": "^4.6.3"
    },
    "devDependencies": {
        "befly-vite": "workspace:*",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=9.0.0"
    },
    "gitHead": "96136321570fd1abf859e691d6ccbc69d7696c4b"
