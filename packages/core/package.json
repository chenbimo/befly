    "files": [
        ".gitignore",
        "befly.config.ts",
        "bunfig.toml",
        "LICENSE",
        "main.ts",
        "package.json",
        "paths.ts",
        "README.md",
        "tsconfig.json",
        "checks/",
        "configs/",
        "docs/",
        "hooks/",
        "lib/",
        "loader/",
        "plugins/",
        "router/",
        "sync/",
        "types/",
        "utils/"
    ],
    "type": "module",
    "main": "main.ts",
    "types": "main.ts",
