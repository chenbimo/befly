{
    "name": "befly-shared",
    "version": "1.4.2",
    "gitHead": "79270d11fb0ff1a5dafada0d12812f4335ef9f45",
    "private": false,
    "description": "Befly 前端/Vite 共享工具包（仅供 admin 与 Vite 工具使用，禁止接口端/服务端导入）",
    "license": "Apache-2.0",
    "author": "chensuiyi <bimostyle@qq.com>",
    "files": [
        "dist/"
    ],
    "type": "module",
    "exports": {
        "./utils/*": {
            "types": "./dist/utils/*.d.ts",
            "default": "./dist/utils/*.js"
        },
        "./types/*": {
            "types": "./dist/types/*.d.ts",
            "default": "./dist/types/*.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "rimraf dist && bunx tsgo -p tsconfig.build.json",
        "test": "bun test",
        "prepack": "bun run build && bun run ./scripts/ensureDist.ts",
        "typecheck": "bunx tsgo -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "es-toolkit": "^1.43.0"
    },
    "engines": {
        "bun": ">=1.3.0"
    }
}
