{
    "name": "befly-tpl",
    "version": "3.5.1",
    "description": "Befly 3.0 TypeScript Template",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "dev": "bun run --env-file=.env.development main.ts",
        "start": "bun run --env-file=.env.production main.ts",
        "pm2": "pm2 start pm2.config.cjs",
        "sync:dev": "cross-env NODE_ENV=development bunx befly sync",
        "sync:prod": "cross-env NODE_ENV=production bunx befly sync"
    },
    "files": [
        "apis",
        "tables",
        "main.ts",
        "ecosystem.config.js",
        ".env.development",
        ".npmrc",
        "befly.service",
        "LICENSE",
        "package.json",
        "pm2.config.cjs",
        "README.md",
        "SYSTEMD_DEPLOY.md",
        "tsconfig.json"
    ],
    "type": "module",
    "dependencies": {
        "befly": "workspace:*"
    },
    "engines": {
        "bun": ">=1.3.0"
    },
    "gitHead": "96136321570fd1abf859e691d6ccbc69d7696c4b",
    "devDependencies": {
        "cross-env": "^10.1.0",
        "dotenv": "^17.2.3"
    }
}
