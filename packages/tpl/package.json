{
    "name": "befly-tpl",
    "version": "3.2.5",
    "main": "main.ts",
    "types": "main.ts",
    "description": "Befly 3.0 TypeScript Template",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "dev": "bun run main.ts",
