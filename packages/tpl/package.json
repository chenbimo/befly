{
    "name": "befly-tpl",
    "version": "3.9.15",
    "description": "Befly 3.0 TypeScript Template",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "dev": "NODE_ENV=development bun run main.ts",
        "start": "NODE_ENV=production bun run main.ts",
        "pm2": "bun --bun pm2 start pm2.config.cjs",
        "sync:api:dev": "NODE_ENV=development bunx befly sync:api",
        "sync:api:prod": "NODE_ENV=production bunx befly sync:api"
    },
    "files": [
        "apis",
        "tables",
        "main.ts",
        ".npmrc",
        "bunfig.toml",
        "env.ts",
        "LICENSE",
        "main.ts",
        "menu.json",
        "package.json",
        "pm2.config.cjs",
        "README.md",
        "tsconfig.json"
    ],
    "type": "module",
    "dependencies": {
        "@befly-addon/admin": "workspace:*",
        "befly": "workspace:*",
        "befly-cli": "workspace:*"
    },
    "engines": {
        "bun": ">=1.3.0"
    },
    "gitHead": "96136321570fd1abf859e691d6ccbc69d7696c4b"
}
