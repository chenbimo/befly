{
    "name": "befly-tpl",
    "version": "3.4.12",
    "description": "Befly 3.0 TypeScript Template",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "dev": "bun run --env-file=.env.development main.ts",
        "start": "bun run --env-file=.env.production main.ts",
        "pm2:start": "pm2 start ecosystem.config.js --env production",
        "pm2:dev": "pm2 start ecosystem.config.js --env development",
        "sync:dev": "NODE_ENV=development befly sync",
        "sync:prod": "NODE_ENV=production befly sync"
    },
    "files": [
        "apis",
        "tables",
        "main.ts",
        "ecosystem.config.js",
        ".env.development",
        ".npmrc",
        "befly.service",
        "LICENSE",
        "package.json",
        "pm2.config.cjs",
        "README.md",
        "SYSTEMD_DEPLOY.md",
        "tsconfig.json"
    ],
    "type": "module",
    "dependencies": {
        "befly": "^3.4.11"
    },
    "engines": {
        "bun": ">=1.3.0"
    },
    "gitHead": "96136321570fd1abf859e691d6ccbc69d7696c4b"
}
