        "package.json",
        "pm2.config.cjs",
        "README.md",
        "tsconfig.json"
    ],
    "type": "module",
    "dependencies": {
        "befly": "workspace:*",
        "befly-cli": "workspace:*"
    },
    "engines": {
        "bun": ">=1.3.0"
